<launch>
   <group ns="top_cam">
      <node pkg="uvc_camera" type="uvc_camera_node" name="camera_node">
         <param name="device" value="/dev/video0"/>
         <param name="fps" value="5"/>
         <param name="width" value="320"/>
         <param name="height" value="240"/>

         <param name="exposure" value="50"/>
         <param name="gain" value="50"/>
         <param name="brightness" value="32"/>
         <param name="wbalanace" value="0"/>
      </node>
   </group>
</launch>
