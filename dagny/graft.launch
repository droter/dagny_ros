<launch>
  <node name="graft_vel" pkg="graft" type="graft_ukf_velocity" output="screen">
    <rosparam file="$(find dagny)/config/graft.yaml" command="load" />
  </node>

  <node name="imu_bias_remover" pkg="imu_pipeline" type="imu_bias_remover">
    <param name="use_odom" value="true"/>
    <param name="accumulator_alpha" value="0.003"/>
  </node>

  <node name="raw_compass" pkg="dagny" type="raw_compass.py"/>

</launch>
